@model LogUser;
<h2 class="pb-2 font-weight-light">Login</h2>
    <form action="/login" method="post">
        <div class="form-group">
            <label for="LogEmail">Email</label>
            <input asp-for="LogEmail" class="form-control" value="s@seagull.com">
            <span asp-validation-for="LogEmail" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label for="LogPassword">Password</label>
            <input asp-for="LogPassword" class="form-control" value="password">
            <span asp-validation-for="LogPassword" class="text-danger"></span>
        </div>
        <input type="submit" value="Login" class="btn btn-success">
    </form>